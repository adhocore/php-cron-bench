{
    "name": "adhocore/cron-bench",
    "description": "Benchmark PHP cron libraries",
    "type": "library",
    "keywords": ["php","php-cron-bench","cron"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jitendra Adhikari",
            "email": "jiten.adhikary@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ahc\\CronBench\\": "src/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "Ahc\\CronBench\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "adhocore/cron-expr": "@stable",
        "mtdowling/cron-expression": "@stable",
        "phpbench/phpbench": "@stable"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        }
    },
    "scripts": {
        "post-root-package-install": [
        ],
        "test": "vendor/bin/phpunit --coverage-text --coverage-clover coverage.xml"
    }
}
